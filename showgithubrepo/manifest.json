{
    "manifest_version": 2,

    "name": "Show Github Repos",
    "description": "Show Github repos in user profile list",
    "version": "1.0",
    "icons": {
        "32": "tt_32.png"
    },
    "default_icon": "tt_32.png",
    "permissions": [
        "https://github.com/"
    ],
    "content_scripts": [{
        "matches": ["https://github.com/*"],
        "js": ["app.js"]
    }]
}